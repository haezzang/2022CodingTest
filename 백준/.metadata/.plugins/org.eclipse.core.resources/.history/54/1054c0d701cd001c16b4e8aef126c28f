package 애너그램;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		String a1 = scan.nextLine();
		String a2 = scan.nextLine();
		char []arr1=new char[a1.length()];
		char []arr2=new char[a1.length()];
		for(int i=0; i<a1.length(); i++) {
			arr1[i]=a1.toUpperCase().charAt(i);
			arr2[i]=a2.toUpperCase().charAt(i);
			
		}
		Arrays.sort(arr1);
		Arrays.sort(arr2);
		String res1=""; 		String res2="";
		for(int i=0; i<a1.length(); i++) {
		res1+=arr1[i];
		res2+=arr2[i];
		}
		System.out.println(res1);	System.out.println(res2);
	
	}

}

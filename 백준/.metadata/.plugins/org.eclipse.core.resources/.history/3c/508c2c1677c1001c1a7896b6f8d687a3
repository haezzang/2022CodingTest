package 가장많은글자;

import java.util.Arrays;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		Scanner scan=new Scanner(System.in);
		
		String str= scan.next();
		str=str.replaceAll("\\s", ""); //공백제거
	


		int []count=new int[26]; //알파벳 담아줄 배열
	
		
		for(int i=0; i<str.length(); i++) {
			int c=str.charAt(i);
			count[c-97]++;
		}
		
		int i=1; //반복문 인덱스
		int key=0; //알파벳 인덱스
		int max=count[0]; //최댓값
		String res=""; //결과값
		
		//최댓값 구하기
		for(i=1; i<count.length; i++) {
			if(max<count[i]) {
				max=count[i];
				key =i;
			}
		}
		
		//처음 최댓값 구하기
		res+=(char)(key+97);
		
		//최댓값이 여러개 일 때
		for(i=1; i<count.length; i++) {
			if(key==i)  continue; 
			
			else if(max==count[i]) {
				res+=(char)(i+97);
			}
		}
		
		
		System.out.println(res);
	}
}

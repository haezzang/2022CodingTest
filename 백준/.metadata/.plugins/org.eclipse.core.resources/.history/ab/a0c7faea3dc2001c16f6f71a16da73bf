package ЦђБе;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
    	int n= scan.nextInt();
    	int max=0;
    	int []s=new int[n];
    	
    	for(int i=0; i<n; i++) {
    		int score=scan.nextInt();
    		s[i]=score;	
    		if(max<s[i]) max=s[i];
    	}
    	
    	
    	
   System.out.println(max);
		
	}

}

package 얲쮩썾쥁;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan = new Scanner(System.in);

		int[] count = new int[26]; // 쮊퀛줡 얶쮅촻 좯찘

	
			String str = scan.nextLine();
			str.toUpperCase();
			for (int i = 0; i < str.length(); i++) {
					count[str.charAt(i) - 'A']++;	
		}

		int i = 1; // 좧줯졽 첉옩쫔
		int key = 0; // 쮊퀛줡 첉옩쫔
		int max = count[0]; // 엄쌹
		char res; // 써쎀쌹

		// 엄쌹 쏮핌
		for (i = 1; i < count.length; i++) {
			if (max < count[i]) {
				max = count[i];
				key = i;
			}
		}
		res=(char)(key + 'A');
		// 칩챻 엄쌹 쏮핌


		// 엄쌹첇 찔잂썁첇절..
		for (i = 1; i < count.length; i++) {
			if (key == i)
				continue;

			else if (max == count[i]) {
				res='?';

			}
		}

		System.out.println(res);

	}

}

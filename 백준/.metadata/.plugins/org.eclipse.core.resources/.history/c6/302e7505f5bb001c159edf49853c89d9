package 더하기사이클;

import java.util.Scanner;
import java.util.StringTokenizer;

public class Main {

	public static void main(String[] args) {

		Scanner scan=new Scanner(System.in);
		
			int num=scan.nextInt();
			int newNum=num;
			int cnt=0;
	
				int left=newNum/10;
				int right=newNum%10;
				int sum=left+right;
				newNum=right+sum;

				cnt++;
	
		

			newNum%=10;
			left=newNum/10;
			right=newNum%10;
			sum=left+right;
			newNum=right+sum;
			System.out.println(newNum);
		
		
			
			

			
		
	}

}

package 숫자의개수;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		
		Scanner scan = new Scanner(System.in);

		int[] count = new int[9]; // 숫자담기

	
			String str = scan.nextLine();

			

			for (int i = 0; i < str.length(); i++) {		
					count[str.charAt(i)]++;	
		}

	
	}

}

package 더하기사이클;

import java.util.Scanner;
import java.util.StringTokenizer;

public class Main {

	public static void main(String[] args) {

		Scanner scan=new Scanner(System.in);
		
			int num=scan.nextInt();
			int newNum=0;
			int cnt=0;
			while(num==newNum) {
				int left=num/10;
				int right=num%10;
				int sum=left+right;
				newNum=right+sum;
				cnt++;
			}
		
			System.out.println(cnt);
			
		
		
			
			

			
		
	}

}

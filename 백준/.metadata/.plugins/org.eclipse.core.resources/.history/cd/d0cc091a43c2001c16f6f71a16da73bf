package _2007³â;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		
		Scanner scan = new Scanner(System.in);



	
			int kg=scan.nextInt();
			int cnt=0;
			int sum=0;
			while(kg==sum) {
				if(kg>sum) { sum+=5; cnt++; 	System.out.println(sum); }
				else { sum+=3; cnt++; 	System.out.println(sum);}
				
			}
			System.out.println(sum);
	

	
	}

}
